  "scripts": {
    "analyzeprod": "cross-env ENABLE_BUNDLE_ANALYZER=true npm run build",
    "build": "cross-env NODE_ENV=production webpack",
    "gen:nessie": "rimraf src/services/nessie/client && openapi-generator-cli generate -i http://localhost:19120/q/openapi -g typescript-fetch -o src/services/nessie/client --additional-properties=supportsES6=true,typescriptThreePlus=true,withoutRuntimeChecks=true",
    "lib:install": "node $EXTERNAL_LIB/libInstall.js",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "lint:js": "eslint --cache ./ ${DREMIO_UI_SRC:-src} --quiet",
    "lint:path": "eslint --cache",
    "postinstall": "([ \"$SKIP_POST\" != \"true\" ] && [ \"$EDITION_TYPE\" == \"\" ] && node ../../../npmPostInstall-oss.js) || true",
    "report:oss": "node licenseCheck.js",
    "start": "node server.js",
    "test": "npm-run-all --print-label --parallel lint:js test:only ts-check",
    "test:base": "cd ../../.. && cross-env BABEL_ENV=test mocha --require ${OLDPWD}/test/index.js",
    "test:coverage": "cross-env BABEL_ENV=test nyc mocha --require ./test/index.js ${DREMIO_UI_TESTS:-$PWD/src/\\**/*-spec.js}",
    "test:coverage-html": "cross-env BABEL_ENV=test nyc mocha --require ./test/index.js ${DREMIO_UI_TESTS:-$PWD/src/\\**/*-spec.js} && nyc report --reporter=html",
    "test:exports": "jest",
    "test:only": "npm run test:base -- ${DREMIO_UI_TESTS:-$PWD/src/\\**/*-spec.js}",
    "test:single": "npm run test:base -- --watch",
    "test:watch": "npm run test:only -- --watch",
    "ts-check": "tsc --noEmit"
  },
