{
  "name": "dremio-ui-lib",
  "version": "1.0.0",
  "description": "Common components and utils for Dremio UI",
  "author": "dremio",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "scripts": {
    "build": "npm-run-all --print-label --parallel dist dist-cjs dist-esm dist-themes",
    "dist": "microbundle-crl --css-modules false --format modern,cjs",
    "dist-cjs": "swc ./components --config-file .config/.swcrc-cjs --out-dir dist-cjs",
    "dist-esm": "swc ./components --config-file .config/.swcrc-esm --out-dir dist-esm",
    "dist-themes": "node scripts/buildThemes.mjs",
    "lint:css": "stylelint themes/**/*",
    "lint:js": "eslint .",
    "manifest.json": "node scripts/generate-manifest.mjs",
    "test": "npm-run-all --print-label --parallel test:js lint:js lint:css",
    "test:js": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "deploy": "gh-pages -d example/build",
    "predeploy": "cd example && yarn install && yarn run build",
    "start": "microbundle-crl watch --no-compress --css-modules false --format modern,cjs"
  },
  "peerDependencies": {
    "@material-ui/core": ">= 4.11.0",
    "formik": ">= ^2.2.6",
    "prop-types": ">= 15.5.10",
    "react": ">= ^16.14.0",
    "react-dom": ">= ^16.14.0",
    "react-intl": ">= 2.3.0"
  },
  "devDependencies": {
    "@material-ui/core": "4.11.0",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.158",
    "@types/react-dom": "^17.0.13",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "formik": "^2.2.6",
    "gh-pages": "^2.2.0",
    "glob": "^7.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "prettier": "~2.5",
    "prop-types": "15.5.10",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-intl": "^2.3.0",
    "react-scripts": "^3.4.1",
    "sass": "^1.49.9",
    "stylelint": "^14",
    "stylelint-config-prettier": "^9",
    "stylelint-config-standard": "^25",
    "stylelint-config-standard-scss": "^3",
    "stylelint-use-logical-spec": "^3",
    "typescript": "^3"
  },
  "files": [
    "dist",
    "dist-themes"
  ],
  "dependencies": {
    "clsx": "^1.1.1",
    "lodash": "4.17.21",
    "react-copy-to-clipboard": "^5.0.3",
    "smart-icon": "^1.4.3",
    "uuid": "^8.3.2"
  }
}
